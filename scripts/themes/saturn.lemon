function void Saturn.TargetTime(u16 medal_px, u16 medal_py, u8 medalOffset)
{
    u32 medal_time_address = 0x840240 + (0x10 * (setting.levelselect.characters - 1)) + medalOffset
    u16 medal_time_minutes = u8[medal_time_address + 1]
    u16 medal_time_seconds = u8[medal_time_address + 2]
    u16 medal_time_frames = u8[medal_time_address + 3]
    u16 medal_time_centiseconds = (medal_time_frames * 99 + 30) / 59

	u8 flags = SPRITE_FLAG_PRIO
	if (u32[0xffffb250] == 0x2d690)	// title card is active
		flags |= SPRITE_FLAG_NO_GLOBAL_TINT	// do not tint while the title card is up. (Should be the same for the main HUD, but that's hidden for longer)

	Renderer.drawCustomSprite(stringformat("saturn_smalldigit_%d", medal_time_minutes % 10), getScreenExtend() - 40 + medal_px, medal_py, 0, flags, 0xe008)
	Renderer.drawCustomSprite("saturn_smallchar_minutes", getScreenExtend() - 40 + medal_px + 0x07, medal_py, 0, flags, 0xe008)
	Renderer.drawCustomSprite(stringformat("saturn_smalldigit_%d", medal_time_seconds / 10), getScreenExtend() - 40 + medal_px + 0x0d, medal_py, 0, flags, 0xe008)
	Renderer.drawCustomSprite(stringformat("saturn_smalldigit_%d", medal_time_seconds % 10), getScreenExtend() - 40 + medal_px + 0x15, medal_py, 0, flags, 0xe008)
	Renderer.drawCustomSprite("saturn_smallchar_seconds", getScreenExtend() - 40 + medal_px + 0x1d, medal_py, 0, flags, 0xe008)
	Renderer.drawCustomSprite(stringformat("saturn_smalldigit_%d", medal_time_centiseconds / 10), getScreenExtend() - 40 + medal_px + 0x25, medal_py, 0, flags, 0xe008)
	Renderer.drawCustomSprite(stringformat("saturn_smalldigit_%d", medal_time_centiseconds % 10), getScreenExtend() - 40 + medal_px + 0x2d, medal_py, 0, flags, 0xe008)
}

