function bool Saturn.TitleCardBar(s16 px, s16 py, s16 renderQueue)
{
	if (objA0.update_address == 0x02d8e2)
	{
		u64 backdropKeyLight = "saturn_titlecard_bluebg_light"
		u64 backdropKeyDark = "saturn_titlecard_bluebg_dark"
		u64 verticalBarKey = "saturn_titlecard_whitebar"
		if (global.zone == 0x0c) // special yellow bar for The Doomsday Zone
		{
			backdropKeyLight = "saturn_titlecard_yellowbg_light"
			backdropKeyDark = "saturn_titlecard_yellowbg_dark"
		}
		else if (global.characters == 2)
		{
			backdropKeyLight = "saturn_titlecard_orangebg_light"
			backdropKeyDark = "saturn_titlecard_orangebg_dark"
		}
		else if (global.characters == 3)
		{
			backdropKeyLight = "saturn_titlecard_greenbg_light"
			backdropKeyDark = "saturn_titlecard_greenbg_dark"
			verticalBarKey = "saturn_titlecard_yellowbar"
		}

		s16 backdropYLight = getScreenHeight()/2 + ((objA0.value32 < 14) ? s16[A0 + 0x32] * getScreenHeight()/14 - getScreenHeight() : 0)
		s16 backdropYDark = getScreenHeight()/2 - ((objA0.value32 < 14) ? s16[A0 + 0x32] * getScreenHeight()/14 - getScreenHeight() : 0)
		s16 backdropXLight = backdropYLight + (getScreenWidth() - 224)/2
		s16 backdropXDark = backdropYDark + (getScreenWidth() - 224)/2
		if (getScreenHeight() == 224)
		{
			backdropYLight -= 8
			backdropYDark += 8
			backdropXLight -= 8
			backdropXDark += 8
		}
		s16 horizontalBarPosition = 9 + getScreenExtend()/2 + ((objA0.value32 < 28) ? getScreenWidth() - (getScreenWidth()/20 * (objA0.value32 - 8)) : 0)
		s16 verticalBarPosition = ((objA0.value32 < 22) ? 120/7 * (objA0.value32 - 7) - 240 : 0)
		if (getScreenHeight() == 224)
			verticalBarPosition -= 16
		if (global.zone == 0x0c)
		{
			// Doomsday loads so fast that the bars can't even make it all the way to their locked positions! We need to speed the bars up a bit.
			horizontalBarPosition = 9 + getScreenExtend()/2 + ((objA0.value32 < 24) ? getScreenWidth() - (getScreenWidth()/20 * (objA0.value32 - 4)) : 0)
			verticalBarPosition = ((objA0.value32 < 18) ? 120/7 * (objA0.value32 - 3) - 240 : 0)
			if (getScreenHeight() == 224)
				verticalBarPosition -= 16		
		}
		s16 animationTimer = objA0.value3e
		u8 alpha = 255

		if (S2TitleCard.disappear)		// We assume that the timer will be at more than 0'02"00 when finishing a level or entering a special/bonus stage
		{
			backdropYLight = getScreenHeight()/2 - ((animationTimer > 4) ? -((animationTimer - 4) * getScreenHeight()/14) : 0)
			backdropYDark = getScreenHeight()/2 + ((animationTimer > 4) ? -((animationTimer - 4) * getScreenHeight()/14) : 0)
			backdropXLight = backdropYLight + (getScreenWidth() - 224)/2
			backdropXDark = backdropYDark + (getScreenWidth() - 224)/2
			horizontalBarPosition = 9 + getScreenExtend()/2 + (getScreenWidth()/8 * max (0, animationTimer - 60))/4
			verticalBarPosition = -(300/7 * animationTimer)/4
			if (getScreenHeight() == 224)
				verticalBarPosition -= 16
		}
		if (S2TitleCard.full)
		{
			Renderer.drawCustomSprite(backdropKeyLight, backdropXLight, backdropYLight, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe009)
			Renderer.drawCustomSprite(backdropKeyDark, backdropXDark, backdropYDark, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe009)
		}
		else
		{
			alpha = 212
		}
		if (objA0.value32 >= 7 || S2TitleCard.disappear)
		{
			Renderer.drawCustomSprite(verticalBarKey, 33 + 3*getScreenExtend()/4, verticalBarPosition, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00a, 0, alpha)
			Renderer.drawCustomSprite("saturn_titlecard_redbar", horizontalBarPosition, getScreenHeight() - 64, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00b, 0, alpha)

			if (unlock_act)
				Renderer.drawCustomSprite(stringformat("saturn_titlecard_unlock%04d", unlock_act), horizontalBarPosition - 197 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 57, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)
			else if (global.zone_act == 0x1600)
				Renderer.drawCustomSprite(stringformat("saturn_titlecard_zone9", global.zone), horizontalBarPosition - 197 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 57, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)
			else if (global.zone_act == 0x1601)
				Renderer.drawCustomSprite(stringformat("saturn_titlecard_zone13", global.zone), horizontalBarPosition - 197 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 57, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)
			else if (global.zone_act == 0x1700)
				Renderer.drawCustomSprite(stringformat("saturn_titlecard_zone11", global.zone), horizontalBarPosition - 197 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 57, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)
			else
				Renderer.drawCustomSprite(stringformat("saturn_titlecard_zone%d", global.zone), horizontalBarPosition - 197 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 57, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)

			if (global.zone_act == 0x1600 || global.zone_act == 0x1700)		// LRZ2 boss, DEZ2 final boss
				Renderer.drawCustomSprite("saturn_titlecard_act2", horizontalBarPosition - 65 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 36, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)
			else if (!unlock_act && global.zone_act != 0x1601 && global.zone != 0x0a && global.zone != 0x0c)	// don't draw anything in an unlockable act, HPZ, SSZ, or DDZ, but prevent the original graphic from showing
				Renderer.drawCustomSprite(stringformat("saturn_titlecard_act%d", global.act.apparent + 1), horizontalBarPosition - 65 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 36, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)

			if (unlock_act)
				Renderer.drawCustomSprite("rtwilogo", horizontalBarPosition - 144 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 118, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)
			else
				Renderer.drawCustomSprite("dagelogo", horizontalBarPosition - 120 - getScreenExtend()/2 + getScreenWidth(), getScreenHeight() - 111, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00c, 0, alpha)
		}

		// Remove fade-in for Saturn theme, where we want to reveal the level in full color behind the title card
		if (level.framecounter == 1 && S2TitleCard.full)	// the frame before the framecounter advances, SetPaletteFadedOut() and SetPaletteToBlack() have run
		{
			copyMemory(0xfffffc00, 0xfffffc80, 0x80)	// above water palette
			copyMemory(0xfffff080, 0xfffff000, 0x80)	// underwater palette (note: fade target and displayed palettes are in the opposite order as they are above water)
			palette_fade.count = 0						// tell the fading functions to fade one entry (this byte is number of colors minus 1), which is always black anyway
		}
		
		return true
	}
}
