		else if (objA0.animation.sprite == 0x02 && global.zone_act != 0x1601 && global.zone != 0x0a && global.zone != 0x0c) // exclude HPZ, SSZ, and DDZ from drawing act numbers
		{
			Renderer.drawCustomSprite(stringformat("saturn_titlecard_act%d", global.act.apparent + 1), px + 6, py, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00b)
			return true
		}
*/
		// Originally the word "zone", now handled in Saturn.TitleCardBar()
		else if (objA0.animation.sprite == 0x03)
		{
//			Renderer.drawCustomSprite("saturn_titlecard_zone_text", px, py, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00b)
			return true
		}
		// Zone name, found by checking which mappings will be used since the actual zone ID order is unwieldy, now handled in Saturn.TitleCardBar()
		else if (objA0.animation.sprite >= 0x04 && objA0.animation.sprite <= 0x11)
		{
//			if (unlock_act)
//				Renderer.drawCustomSprite(stringformat("saturn_titlecard_unlock%04d", unlock_act), px, py, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00b)
//			else
//				Renderer.drawCustomSprite(stringformat("saturn_titlecard_zone%d", objA0.animation.sprite - 4), px, py, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00b)
			return true
		}
		// "Bonus"
		else if (objA0.animation.sprite == 0x12 || objA0.animation.sprite == 0x13)
		{
			Renderer.drawCustomSprite("saturn_titlecard_bonus", px, py, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe00b)
			return true
