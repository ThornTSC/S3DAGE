function void Saturn.HUD.drawSprite(u64 key, u16 px, u16 py, u16 renderQueue)
{
	if (hud.layout == 2 && (py == 0x28 || (py == 0x08 && unlock_act == s3_CGZ) || (py == 0x08 && global.zone >= 0x13 && global.zone <= 0x15)) && (key != "saturn_hud_lives_icon_sonic" && key != "saturn_hud_lives_icon_tails" && key != "saturn_hud_lives_icon_miles" && key != "saturn_hud_lives_icon_knuckles"))
	{
		bool showRingHUD = (ring_counter != 0 || (level.framecounter & 0x08))
		u32 alpha = 0xffffffff
		if (!showRingHUD)
			alpha = 0xff0000ff
		if (py == 0x28)
		{
			px -= 64
			py -= 24
		}
		else
		{
			px -= 40
			py += 8
		}
		Renderer.drawCustomSprite(key, px, py, 0, SPRITE_FLAG_PRIO, renderQueue + 1, 0, alpha)	// -74, -30 centers display atop the ring
		return
	}

    u16 inputY = py
  	if ((time_attack == 1 || time_attack == 2) && py != 0x48 && !HUD.isShadowTarget(key))	// 0x48 is the height of Shadow's boost bar
	{
        py -= 0x10
		if (time_attack_hud)	// center HUD?
		{
			//px += 136			// move time and rings to the right, half a pixel left of center
			px += getScreenWidth() / 2 - 64
			if (inputY == 0x28 && hud.layout == 1)	// rings?
				px += 13		// move even more to the right. Note that this is 1px more than it should be for aesthetics, lining up TIME's M with RINGS' R.
		}
	}
	if (px >= 0x58 && px <= 0x68 && py == 0x28 && hud.layout == 1)	// Rings in Mania HUD position
		px -= 0x18								// move to classic position

	u8 flags = SPRITE_FLAG_PRIO
	if (u32[0xffffb250] == 0x2d690)	// title card is active, rendering lives text/icon/numbers in either classic or mobile positions
		flags |= SPRITE_FLAG_NO_GLOBAL_TINT	// do not tint the HUD while the title card is up.

	Renderer.drawCustomSprite(key, px, py, 0, flags, renderQueue)
	if (inputY == 0x18 && freeze_time)	// HUD graphics have a base py of 0x08, and the TIME row is 0x10 beyond that
    {
		if (freeze_time >= 64)
		{
			if (key == "saturn_hud_char_minutes")
				Renderer.drawCustomSprite("saturn_hud_char_minutes_chaosstage", px, py, 0, flags, renderQueue + 1, 0, 255)
			else if (key == "saturn_hud_char_seconds")
				Renderer.drawCustomSprite("saturn_hud_char_seconds_chaosstage", px, py, 0, flags, renderQueue + 1, 0, 255)
			else if (key == "saturn_hud_char_colon")
				Renderer.drawCustomSprite("saturn_hud_char_colon_chaosstage", px, py, 0, flags, renderQueue + 1, 0, 255)
			else if (key == "saturn_hud_text_time")
				Renderer.drawCustomSprite("saturn_hud_text_time_chaosstage", px, py, 0, flags, renderQueue + 1, 0, 255)
		}
		else
		{
			if (key == "saturn_hud_char_minutes")
				Renderer.drawCustomSprite("saturn_hud_char_minutes_chaosstage", px, py, 0, flags, renderQueue + 1, 0, freeze_time * 4)
			else if (key == "saturn_hud_char_seconds")
				Renderer.drawCustomSprite("saturn_hud_char_seconds_chaosstage", px, py, 0, flags, renderQueue + 1, 0, freeze_time * 4)
			else if (key == "saturn_hud_char_colon")
				Renderer.drawCustomSprite("saturn_hud_char_colon_chaosstage", px, py, 0, flags, renderQueue + 1, 0, freeze_time * 4)
			else if (key == "saturn_hud_text_time")
				Renderer.drawCustomSprite("saturn_hud_text_time_chaosstage", px, py, 0, flags, renderQueue + 1, 0, freeze_time * 4)
		}
    }
}

function void Saturn.HUD.drawDigit_Challenges(u8 digit, u16 px, u16 py, u16 renderQueue)
{
    u16 inputY = py
    if (time_attack == 1 || time_attack == 2)
	{
        py -= 0x10
		if (time_attack_hud)	// center HUD?
		{
			// px += 136			// move time and rings to the right, half a pixel left of center
			px += getScreenWidth() / 2 - 64
			if (inputY == 0x28 && hud.layout == 1)	// rings?
				px -= 12		// move to the left
		}
	}

	u8 flags = SPRITE_FLAG_PRIO
	if (u32[0xffffb250] == 0x2d690)	// title card is active
		flags |= SPRITE_FLAG_NO_GLOBAL_TINT	// do not tint while the title card is up. (Should be the same for the main HUD, but that's hidden for longer)

	Renderer.drawCustomSprite(stringformat("saturn_hud_digit_%d", digit), px, py, 0, flags, renderQueue)
	if (inputY == 0x18 && freeze_time)	// HUD graphics have a base py of 0x08, and the TIME row is 0x10 beyond that
		if (freeze_time >= 64)
			Renderer.drawCustomSprite(stringformat("saturn_hud_digit_%d_chaosstage", digit), px, py, 0, flags, renderQueue + 1, 0, 255)
		else
			Renderer.drawCustomSprite(stringformat("saturn_hud_digit_%d_chaosstage", digit), px, py, 0, flags, renderQueue + 1, 0, freeze_time * 4)
}

