// This function sets the manner and direction title card text and act numbers use to leave the screen.
// For this theme, we swap the directions.
// It isn't truly accurate to Sonic 1, but it prevents an early unload due to some display offsets.
function void South.TitleCard.TextElement.Update()
{
	A1 = 0xffff0000 + u16[A0 + 0x48]
	D0 = u16[A1 + 0x32]
	if (D0.u16 != 0)
	{
		if (!(objA0.render_flags & render_flag.VISIBLE))
		{
			--u16[A1 + 0x30]
			UnloadObject()
			return
		}

		if (D0.u8 >= u8[A0 + 0x28])
		{
			objA0.position.x.u16 -= 0x20
		}
	}
	else
	{
		// Appearing or showing
		if (objA0.position.x.u16 > u16[A0 + 0x46])
		{
			objA0.position.x.u16 = max(objA0.position.x.u16 - 0x10, u16[A0 + 0x46])
			u8[A1 + 0x34] = 0xff
		}
	}
	DrawObject()
}

// Used for title card bar and text functions
constant array<s16> center_offset =
{
	60, // AIZ
	84, // HCZ
	48, // MGZ
	48, // CNZ
	44, // FBZ
	108, // ICZ
	64, // LBZ
	52, // MHZ
	76, // SOZ
	80, // LRZ
	48, // SSZ
	80, // DEZ
	56, // DDZ
	52  // HPZ
}

function bool South.TitleCardBar(s16 px, s16 py, s16 renderQueue)
{
	if (objA0.update_address == 0x02d8e2)
	{
		u64 barKey = "south_titlecard_bluebar"
		if (global.zone == 0x0c) // special yellow bar for The Doomsday Zone
			barKey = "south_titlecard_yellowbar"
		else if (global.characters == 2)
			barKey = "south_titlecard_orangebar"
		else if (global.characters >= 3)
			barKey = "south_titlecard_redbar"

		// We call this via Zone ID later to center-align per zone.
		u8 index = global.zone
		if (global.zone_act == 0x1601 || global.zone_act == 0x1701)
			index = 0x0d	// HPZ
		else if (global.zone_act == 0x1700)
			index = 0x0b	// DEZ final bosses
		else if (global.zone_act == 0x1600)
			index = 0x09	// LRZ2 boss
		else if (global.zone >= 0x0d && global.zone <= 0x15)	// this shouldn't happen!
			index = 0x00	// AIZ as a fallback

		s16 resolution_tweak = getScreenWidth()/2 - 200

		// px and py are rearranged here to create a fly-in from the right instead of the top
		Renderer.drawCustomSprite(barKey, 400 + resolution_tweak - py - 0x24 - center_offset[index], px - 0x24 - 0x20 - resolution_tweak, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
		return true
	}
}
function bool South.TitleCardText(s16 px, s16 py, s16 renderQueue)
{
	// Sonic 1 center-aligns zone names (with exceptions for the pixel not on the right side of most letters) 20 pixels below the RINGS display.
	// Next, the word ZONE is aligned such that it ends 16 pixels before the last letter of the zone name (again accounting for the two empty pixels after an E).
	// The oval and act number maintain their alignment relative to the word ZONE.

	// We call this via Zone ID later to center-align per zone.
	u8 index = global.zone
	if (unlock_act)
		index = 0x00	// EHZ name length matches that of AIZ
	else if (global.zone_act == 0x1601 || global.zone_act == 0x1701)
		index = 0x0d	// HPZ
	else if (global.zone_act == 0x1700)
		index = 0x0b	// DEZ final bosses
	else if (global.zone_act == 0x1600)
		index = 0x09	// LRZ2 boss
	else if (global.zone >= 0x0d && global.zone <= 0x15)	// this shouldn't happen!
		index = 0x00	// AIZ as a fallback

	// Next, code for zone and act names.
	if (objA0.update_address == 0x02d95c && !original_mode)
	{
		// Act number, found by checking which mappings will be used since the actual zone ID order is unwieldy.
		if (objA0.animation.sprite == 0x02 && unlock_act)
		{
			return true	// don't draw anything but prevent the original graphic from showing
		}
		if (objA0.animation.sprite == 0x02 && global.zone_act == 0x1600) // force "Act 2" to show in the LRZ2 boss level
		{
			Renderer.drawCustomSprite("south_titlecard_act2", px + 8 - center_offset[index], py - 0x22 - 0x20, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			return true
		}
		else if (objA0.animation.sprite == 0x02 && global.zone_act == 0x1700) // force "Act 2" to show in the DEZ2 final boss level
		{
			Renderer.drawCustomSprite("south_titlecard_act2", px + 8 - center_offset[index], py - 0x22 - 0x20, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			return true
		}
		else if (objA0.animation.sprite == 0x02 && global.zone_act != 0x1601 && global.zone != 0x0a && global.zone != 0x0c) // exclude HPZ, SSZ, and DDZ from drawing act numbers
		{
			Renderer.drawCustomSprite(stringformat("south_titlecard_act%d", global.act.apparent + 1), px + 8 - center_offset[index], py - 0x22 - 0x20, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			return true
		}
		// Originally the word "zone", now white bars
		else if (objA0.animation.sprite == 0x03)
		{
			Renderer.drawCustomSprite("south_titlecard_zone_text", px - 0x10 - center_offset[index], py - 0x04 - 0x20, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			return true
		}
		// Zone name, found by checking which mappings will be used since the actual zone ID order is unwieldy
		else if (objA0.animation.sprite >= 0x04 && objA0.animation.sprite <= 0x11)
		{
			if (unlock_act)
				Renderer.drawCustomSprite(stringformat("south_titlecard_unlock%04d", unlock_act), getScreenWidth() - px - center_offset[index], py - 0x20, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			else
				Renderer.drawCustomSprite(stringformat("south_titlecard_zone%d", objA0.animation.sprite - 4), getScreenWidth() - px - center_offset[index], py - 0x20, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			return true
		}
		// "Bonus"
		else if (objA0.animation.sprite == 0x12 || objA0.animation.sprite == 0x13)
		{
			Renderer.drawCustomSprite("south_titlecard_bonus", px, py, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			return true
		}
		// "Stage"
		else if (objA0.animation.sprite == 0x14)
		{
			Renderer.drawCustomSprite("south_titlecard_stage", px, py, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, renderQueue + 0x4000)
			return true
		}
	}
}


function string South.getCharacterBonusTextIcon(u8 character)
{
	if ((global.game_mode & 0x7f) == 0x48)
		return "south_hud_bonus_icon_special"
	return (character == CHARACTER_SONIC) ? "south_hud_bonus_icon_blue" : (character == CHARACTER_TAILS) ? "south_hud_bonus_icon_orange" : "south_hud_bonus_icon_red"
}

function bool South.LevelResults(s16 px, s16 py, s16 renderQueue)
{
    if (objA0.animation.sprite == 0x0b)
    {
        Renderer.drawCustomSprite(South.getCharacterBonusTextIcon(getMainCharacter()), px+37, py-5, 0, SPRITE_FLAG_PRIO, 0xe000)  // these icons are blank in this theme

        Renderer.drawCustomSprite("south_hud_text_total", px, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x0c)
    {
        Renderer.drawCustomSprite(South.getCharacterBonusTextIcon(getMainCharacter()), px+37, py-5, 0, SPRITE_FLAG_PRIO, 0xe000)  // these icons are blank in this theme

        Renderer.drawCustomSprite("south_hud_text_bonus", px, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x0d)
    {
        Renderer.drawCustomSprite("south_hud_text_ring", px, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x0e)
    {
        Renderer.drawCustomSprite("south_hud_text_time", px, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x13)
    {
        Renderer.drawCustomSprite("south_sonic_text", px + 5, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x15)
    {
        Renderer.drawCustomSprite("south_tails_text", px - 3, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x14)
    {
        Renderer.drawCustomSprite("south_miles_text", px + 5, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    if (objA0.animation.sprite == 0x16)
    {
        Renderer.drawCustomSprite("south_knuckles_text", px + 25, py, 0, SPRITE_FLAG_PRIO, 0xe008) // large horizontal shift for Knuckles to center KNUCKLES GOT
        return true
    }
    else if (objA0.animation.sprite == 0x11)
    {
        if (isMainCharacter(CHARACTER_KNUCKLES))
            px += 28

        Renderer.drawCustomSprite("south_got_text", px - 5, py, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x10)
    {
        s16 through_x_offset = (global.zone_act == 0x0a00 || unlock_act) ? -40 : 0		// show "Zone" in SSZ and unlockable acts, thus shift "Through" left.
        Renderer.drawCustomSprite("south_through_text", px + through_x_offset, py, 0, SPRITE_FLAG_PRIO, 0xe008)

		// Render the title card oval
		u64 barKey = "south_titlecard_bluebar"
		if (global.characters == 2)
			barKey = "south_titlecard_orangebar"
		else if (global.characters >= 3)
			barKey = "south_titlecard_redbar"
	
		Renderer.drawCustomSprite(barKey, px + through_x_offset + 0x50, py - 0x18, 0x00, SPRITE_FLAG_PRIO | SPRITE_FLAG_NO_GLOBAL_TINT, 0xe008 - 1)
        return true
    }
    else if (objA0.animation.sprite == 0x0f && (global.zone_act == 0x0a00 || unlock_act))	// show "Zone" in SSZ and unlockable acts.
    {
        Renderer.drawCustomSprite("south_titlecard_zone_text", px - 28, py + 16, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
    else if (objA0.animation.sprite == 0x0f)
    {
        Renderer.drawCustomSprite(stringformat("south_titlecard_act%d", global.zone_act == 0x1600 ? 2 : global.act.apparent + 1), px-32, py+18, 0, SPRITE_FLAG_PRIO, 0xe008)
        return true
    }
}

function bool South.SpecialStageResults(s16 px, s16 py, s16 renderQueue)
{
	u8 gameMode = global.game_mode & 0x7f
	if (objA0.update_address == 0x02ea50 || objA0.update_address == 0x02eac8 || objA0.update_address == 0x02ebcc || objA0.update_address == 0x02ec1e && (gameMode == 0x48 || gameMode == 0x0c))
	{
		s16 x_offset = 0
		s16 y_offset = 0

		bool isSKStage = (global.lock_on_state == 0 && global.sk_bluespheres)

        // HUD.drawNumber() and HUD.drawSprite() calls get funneled through a function that appends the theme prefix. Do NOT add it manually!
        // This also means using the standard getCharacterBonusTextIcon() function instead of the theme-specific one.
		if (objA0.animation.sprite == 0x17 || objA0.animation.sprite == 0x31 || objA0.animation.sprite == 0x36)
		{
			HUD.drawSprite("hud_bonus_icon_special", px-60, py-5, renderQueue-1)
				
			HUD.drawSprite(isSKStage ? "hud_text_score_superstage" : "hud_text_score_chaosstage", px - 96, py, renderQueue)
			HUD.drawNumber(isSKStage ? "hud_digit_%d" : "hud_digit_%d_chaosstage", player.score * 10, px + 88, py, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x18 || objA0.animation.sprite == 0x32 || objA0.animation.sprite == 0x37)
		{
			HUD.drawSprite("hud_bonus_icon_special", px+77, py-5, renderQueue-1)

			HUD.drawSprite(isSKStage ? "hud_text_ring_superstage" : "hud_text_ring_chaosstage", px, py, renderQueue)
			HUD.drawSprite(isSKStage ? "hud_text_bonus_superstage" : "hud_text_bonus_chaosstage", px + 40, py, renderQueue)
			HUD.drawNumber(isSKStage ? "hud_digit_%d" : "hud_digit_%d_chaosstage", results.ring_bonus * 10, px + 184, py, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x19 || objA0.animation.sprite == 0x33 || objA0.animation.sprite == 0x38)
		{
			HUD.drawSprite("hud_bonus_icon_special", (unlock_act) ? px + 93 : px+52, py-5, renderQueue-1)
			
			if (!unlock_act)
				HUD.drawSprite(isSKStage ? "hud_text_perfect_superstage" : "hud_text_perfect_chaosstage", px, py, renderQueue)
			else
			{
				HUD.drawSprite(isSKStage ? "hud_text_sphere_superstage" : "hud_text_sphere_chaosstage", px, py, renderQueue)
				HUD.drawSprite(isSKStage ? "hud_text_bonus_superstage" : "hud_text_bonus_chaosstage", px + 56, py, renderQueue)
			}
			HUD.drawNumber(isSKStage ? "hud_digit_%d" : "hud_digit_%d_chaosstage", results.time_bonus * 10, px + 184, py, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x1a || objA0.animation.sprite == 0x34 || objA0.animation.sprite == 0x39)
		{
			HUD.drawSprite("hud_bonus_icon_special", px+59, py-5, renderQueue-1)
			
			HUD.drawSprite(isSKStage ? "hud_text_continue_superstage" : "hud_text_continue_chaosstage", px, py, renderQueue)
			return true
		}

		else if (objA0.animation.sprite == 0x12)
		{
			x_offset = 0
			if (getMainCharacter() == CHARACTER_TAILS)
				x_offset = (global.region_code & 0x80) ? -1 : 3
			else if (getMainCharacter() == CHARACTER_KNUCKLES)
				x_offset = 3

			u64 getSuperOrHyper = (isSKStage) ? "hyper" : "super"
			if (getMainCharacter() == CHARACTER_TAILS && !Game.getSetting(SETTING_HYPER_TAILS))
				getSuperOrHyper = "super"
			
			u64 getCharacter = "sonic"
			if (getMainCharacter() == CHARACTER_TAILS)
				getCharacter = "tails"
			else if (getMainCharacter() == CHARACTER_KNUCKLES)
				getCharacter = "knuckles"

			Renderer.drawCustomSprite(stringformat("south_%s_text_%s", getSuperOrHyper, getCharacter), px + x_offset, py + y_offset, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x13)
		{
			x_offset = 1
			if (py == 0x0030 || py == 0x00bc)	// SONIC in BE SUPER/HYPER SONIC
				x_offset = 21
			else if (px == 0x0060)	// SONIC in SONIC CAN GO TO
				x_offset = -3
			else if (px == 0x00a8)	// SONIC in NOW SONIC CAN
				x_offset = 5
			
			Renderer.drawCustomSprite("south_sonic_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x14)
		{
			x_offset = 1
			if (py == 0x0030 || py == 0x00bc)	// MILES in BE SUPER/HYPER MILES
				x_offset = 21
			else if (px == 0x0060)	// MILES in MILES CAN GO TO
				x_offset = -3
			else if (px == 0x00a8)	// MILES in NOW MILES CAN
				x_offset = 5
			
			Renderer.drawCustomSprite("south_miles_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x15)
		{
			x_offset = -3
			if (py == 0x0030 || py == 0x00bc)	// TAILS in BE SUPER/HYPER TAILS
				x_offset = 17
			else if (px == 0x0064)	// TAILS in TAILS CAN GO TO
				x_offset = -7
			else if (px == 0x00ac)	// TAILS in NOW TAILS CAN
				x_offset = -1
			
			Renderer.drawCustomSprite("south_tails_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x16)
		{
			x_offset = -4
			if (py == 0x0030 || py == 0x00bc)	// KNUCKLES in BE SUPER/HYPER KNUCKLES
				x_offset = 16
			else if (px == 0x0048)	// KNUCKLES in KNUCKLES CAN GO TO	// likely needs a new value!
				x_offset = -8
			else if (px == 0x0090)	// KNUCKLES in NOW KNUCKLES CAN		// likely needs a new value!
				x_offset = 0
			
			Renderer.drawCustomSprite("south_knuckles_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
		//	debugLog(stringformat("%x", px))	// implemented to help with showing the new px check values
			return true
		}

		else if (objA0.animation.sprite == 0x22)
		{
			Renderer.drawCustomSprite(isSKStage ? "south_specialstage_text_super" : "south_specialstage_text", px, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			Renderer.drawCustomSprite("south_titlecard_yellowbar", getScreenWidth() - px - 32, py - 19, 0, SPRITE_FLAG_PRIO, renderQueue - 0x1000)
			return true
		}
		else if (objA0.animation.sprite == 0x23)
		{
			x_offset = 0
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = 4
			else if (getMainCharacter() == CHARACTER_KNUCKLES)	// offset less for GOT ALL
				x_offset = (px == 0x00d4) ? 3 : 4
			Renderer.drawCustomSprite(isSKStage ? "south_gota_text_super" : "south_gota_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)

			u16 oval_offset = 0
			if ((global.chaos_emeralds == 7 && !isSKStage) || (global.super_emeralds == 7 && isSKStage))	// if GOT A is part of GOT ALL, align the yellow oval differently
				oval_offset = 12
			if (getMainCharacter() == CHARACTER_KNUCKLES)
				oval_offset -= 28
			Renderer.drawCustomSprite("south_titlecard_yellowbar", getScreenWidth() - px - 24 - oval_offset, py - 8, 0, SPRITE_FLAG_PRIO, renderQueue - 0x1000)	// subtract 12 more if part of GOT ALL
			return true
		}
		else if (objA0.animation.sprite == 0x24)
		{
			Renderer.drawCustomSprite("south_chaosemerald_text", px, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x25)
		{
			x_offset = 0
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = 4
			else if (getMainCharacter() == CHARACTER_KNUCKLES)
				x_offset = 3
			Renderer.drawCustomSprite(isSKStage ? "south_gotall_text_super" : "south_gotall_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
		//	Renderer.drawCustomSprite("south_titlecard_yellowbar", getScreenWidth() - px - 30, py - 8, 0, SPRITE_FLAG_PRIO, renderQueue - 0x1000)
			return true
		}
		else if (objA0.animation.sprite == 0x26)
		{
			Renderer.drawCustomSprite(isSKStage ? "south_superemeralds_text_super" : "south_chaosemeralds_text", px, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x27)
		{
			x_offset = -8
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = -12
			Renderer.drawCustomSprite("south_now_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x28)
		{
			x_offset = -16
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = -20
			Renderer.drawCustomSprite("south_be_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x2c)
		{
			x_offset = -8
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = -12
			Renderer.drawCustomSprite("south_now_text_super", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x2d)
		{
			x_offset = 32
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = 36
			Renderer.drawCustomSprite("south_can_text_super", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x2e)
		{
			// No x_offset for Sonic
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = 4
			Renderer.drawCustomSprite("south_cangoto_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x2f)
		{
			Renderer.drawCustomSprite("south_hiddenpalace_text", px, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x30)
		{
			Renderer.drawCustomSprite("south_superemerald_text_super", px, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x35)
		{
			x_offset = -16
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = -20
			Renderer.drawCustomSprite("south_be_text_super", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
		else if (objA0.animation.sprite == 0x3a)
		{
			x_offset = 32
			if (getMainCharacter() == CHARACTER_TAILS && (global.region_code & 0x80))	// if region code check is true, using TAILS instead of MILES
				x_offset = 36
			Renderer.drawCustomSprite("south_can_text", px + x_offset, py, 0, SPRITE_FLAG_PRIO, renderQueue)
			return true
		}
    return false    // update address 0x02ec1e, animation sprite 1 is Super Emerald activation and is not renderhooked
	}
}

function bool South.SpecialStageResultsContinue(s16 px, s16 py, s16 renderQueue)
{
	bool prioFlag = (objA0.sprite_attributes & 0x8000) != 0
	if (global.game_mode == 0x48 && global.traded_emeralds)
		prioFlag = true
	string key = getCharacterContinueIcon(getMainCharacter())
	Renderer.drawCustomSprite(key, px, py, 0x00, prioFlag ? SPRITE_FLAG_PRIO : 0, renderQueue)
	return true
}

function bool South.GameTimeOver(s16 px, s16 py, s16 renderQueue)
{
	if (objA0.animation.sprite == 0)
	{
		Renderer.drawCustomSprite("south_game", px - 72, py - 8, 0, SPRITE_FLAG_PRIO, renderQueue)
		return true
	}
	else if (objA0.animation.sprite == 1 || objA0.animation.sprite == 3)	// first goes with GAME, second with TIME
	{
		Renderer.drawCustomSprite("south_over", px + 8, py - 8, 0, SPRITE_FLAG_PRIO, renderQueue)
		return true
	}
	else if (objA0.animation.sprite == 2)
	{
		Renderer.drawCustomSprite("south_time", px - 72, py - 8, 0, SPRITE_FLAG_PRIO, renderQueue)
		return true
	}
	else
		return false
}

function void South.giantRingCounter(u16 px, u16 py, u16 renderQueue, u8 giantRingsCollected.number, u8 maxGiantRings)
{
	s16 y_offset = (getScreenWidth() - 400)/16	// we base the y position on TOTAL text x value, which changes with screen width at a rate of deltaY/deltaX = -1/16.
	Renderer.drawCustomSprite("hud_giantring", px - 28, py - y_offset - 7, 0x00, SPRITE_FLAG_PRIO, renderQueue)
	u32 tint = (giantRingsCollected.number == maxGiantRings) ? 0xff00ffff : 0xffffffff
	Renderer.drawCustomSprite("south_hud_multiply", px - 1, py - y_offset - 2, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, tint, 0x10000)
	Renderer.drawCustomSprite(stringformat("south_hud_digit_%d", giantRingsCollected.number), px + 6, py - y_offset - 2, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, tint, 0x10000)
	Renderer.drawCustomSprite("south_hud_slash", px + 14, py - y_offset - 2, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, tint, 0x10000)
	Renderer.drawCustomSprite(stringformat("south_hud_digit_%d", maxGiantRings), px + 20, py - y_offset - 2, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, tint, 0x10000)
	Renderer.drawCustomSprite("mgz_pillar_knuxshading", px, py - y_offset + 29, 0x00, SPRITE_FLAG_PRIO, renderQueue - 1, 0, 180)
}
