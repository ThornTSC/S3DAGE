		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + line, "Deborah McCracken", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + 2*line, "Tatsuo Yamada", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + 3*line, "Rick Macaraeg", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + 4*line, "Locky P", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + 5*line, "Masaaki Kawamura", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)
	}
	else if (credits.screen.current == 38)
	{
		if (credits.screen.time_until_fade_out < credits.fade.duration)
			alpha.logo = alpha.text
		else
			alpha.logo = 255
		Renderer.drawCustomSprite("sonic2logo", credits.text.center.x - 60, 6, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, alpha.logo)

		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y - line/2, "Original Game Presented By", 0xffff8000 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + line/2, "SEGA", 0x40a0ff00 + alpha.text, 2, 0, renderQueue, 0, 1)
	}
}
