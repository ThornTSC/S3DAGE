		CreditsDataGenerator.addLine(line+5, 0, "SAXMAN")
		CreditsDataGenerator.addLine(line+6, 0, "THE SONIC RETRO")
		CreditsDataGenerator.addLine(line+7, 0, "COMMUNITY")
