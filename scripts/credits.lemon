		CreditsDataGenerator.addSignal(0xfff4)
	}

	A0 = backupA0
	A1 = backupA1
	return startAddress
}

// Print D.A. Garden Edition credits.
// Function taken from maingame\outro\outro_shared.lemon
//# address-hook(0x05b204) end(0x05b282)
function void fn05b204()
{
	u16 oldCamPos = camera.position.y >> 16
	camera.position.y += outro.scroll_speed
	u16 newCamPos = camera.position.y >> 16

	if ((oldCamPos & 0x0010) != (newCamPos & 0x0010))
	{
		// Next line of text
		D0.u16 = (newCamPos << 4) + 0x0e00
		D1.u16 = D0.u16 & 0x0f00
		fn05b2d6()		// Clear VRAM first for the text line

	#if STANDALONE
		A1 = CreditsDataGenerator.build(u16[0xfffffaae] / 4)
	#else
		u32 address = 0x05b284
		address += u16[0xfffffaae]		// Credits table index (times 4), this is either 0 or 4
		address += (global.lock_on_state == 0) ? 0 : 8	// S3&K credits has some more entries, especially in the music sections
		A1 = u32[address]
	#endif

		// u16[0xfffffa84] is the line index (times 2) of the next text to print
		u16 lineIndex = u16[0xfffffa84]
		A1 = tableLookupAddress(A1, lineIndex)

		// D2.u16 encodes:
		//  - Upper byte: Credits line number when to print the text (empty lines until this is reached)
		//  - Bits 1..7:  X-position in patterns where to start the text and color
		//  - Bit 0:      Text color: white or yellow
		D2.u16 = u16[A1]
		A1 += 2

		if (D2.u16 == 0xfff8)
		{
			// Switch to next credits table
			//  -> There are two of them, as each is limited to 0x100 lines
			//  -> It's even three for S3AIR
			fn05b294()
		}
		else if (D2.u16 == 0xfff4)
		{
			fn05b29e()
		}
		else
		{
			// D0.u16 counts up here until reaching D3.u16, producing empty lines in between
			D0.u16 &= 0xff00
			D3.u16 = D2.u16 & 0xff00
			if (D3.u16 == D0.u16)
			{
				// Next actual text line
				D6.u16 = (D2.u16 & 0x0001) ? (sprite_attribute.PRIORITY | sprite_attribute.PALETTE.LINE1 | (ContinueScreen.TextTiles.targetInVRAM >> 5)) : (sprite_attribute.PRIORITY | (ContinueScreen.TextTiles.targetInVRAM >> 5))	// Includes base sprite VRAM address + the palette to use
				D2.u16 &= 0x0ffe	// This is the start offset from the left side inside plane A

				// Print next characters, read from address A1 (using ASCII encoding)
				if (original_mode)
					fn05b318()
				u16[0xfffffa84] += 2
			}
		}
	}

	if (!original_mode)
		Credits.DAGE()
}

function void Credits.DAGE()
{
	// Insert custom credits here.

	// The Sonic/Tails bad ending credits in Sonic 3 & Knuckles start at level.framecounter = 0x033f.
	// Starting at level.framecounter = 0x033f, show credits that fade in over one second, stay for a few seconds, fade out over one second, and then move to the next screen.
	// Logos for each credited game fade in at the top of the screen over one second, stay for the length of all their credits, and then fade out over one second.

	u16 renderQueue = 0xe000

	// Set timing values. These should actually be different if using emulated music, but everything is close enough to leave as is.
	// Ending type:                                                                          K Bad    K Good                                                            T Bad                                  T Good   T Best                                S Bad                                  S Good   S Best
	u16 credits.start_frame =     (global.zone_act == 0x0a01) ? ((outro.ending_type == -2) ? 0x005c : 0x002f) : (global.characters == 2) ? ((outro.ending_type == -2) ? 0x0325 : (outro.ending_type == 0x00) ? 0x0325 : 0x02dd) : (outro.ending_type == -2) ? 0x033f : (outro.ending_type == 0x00) ? 0x02d7 : 0x02d7
	u16 credits.screen.duration = 558 - (outro.scroll_speed * 3 / 640)						// how long each screen lasts. Formula derived via linear regression of scroll speed vs. max possible frames per screen (via trial and error) for Knux and Sonic bad endings.
	u16 credits.fade.duration = 60															// how long each screen spends fading in and out

	u16 credits.screen.current = (level.framecounter - credits.start_frame) / credits.screen.duration
	u16 credits.screen.time_on_screen = level.framecounter - (credits.start_frame + (credits.screen.current * credits.screen.duration))
	u16 credits.screen.time_until_fade_out = credits.screen.duration - credits.screen.time_on_screen	// how long after initial appearance each screen will wait to start fading out

	u32 alpha.text = (credits.screen.time_on_screen >= credits.fade.duration) ? ((credits.screen.time_until_fade_out < credits.fade.duration) ? (credits.screen.time_until_fade_out * 255 / credits.fade.duration) : 255) : (credits.screen.time_on_screen * 255 / credits.fade.duration)
	u32 alpha.logo

	u16 credits.text.center.x = (global.zone_act == 0x0a01) ? getScreenWidth()/2 : getScreenWidth() - 80 		// getScreenWidth()/2 if centering credits
	u16 credits.text.center.y = (getScreenHeight() - 40) / 2 + 40		// 40s here compensate for space occupied by game logos and extra spacing
	u16 line = 14														// distance between top of consecutive text lines


	if (credits.screen.current == 0)	// start of Sonic 3 A.I.R. D.A. Garden Edition credits
	{
		if (credits.screen.time_on_screen < credits.fade.duration)
			alpha.logo = alpha.text
		else
			alpha.logo = 255

		Renderer.drawCustomSprite("sonic3air_logo", credits.text.center.x, 20, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, alpha.logo)
		if (palette_set >= 2)
			Renderer.drawCustomSprite("title_screen_air_new", credits.text.center.x, 20 + 20, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, alpha.logo)
		else
			Renderer.drawCustomSprite("title_screen_air", credits.text.center.x, 20 + 20, 0x00, SPRITE_FLAG_PRIO, renderQueue, 0, alpha.logo)

		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y - 2*line - line/2, "Project Lead", 0xffff8000 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y - line - line/2, "Thorn", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)

		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + line/2, "in correspondence with", 0xe8d49400 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + line + line/2, "and based upon work by", 0xe8d49400 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y + 2*line + line/2, "D.A. Garden", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)
	}
	else if (credits.screen.current == 1)
	{
		Renderer.drawCustomSprite("sonic3air_logo", credits.text.center.x, 20, 0x00, SPRITE_FLAG_PRIO, renderQueue)
		if (palette_set >= 2)
			Renderer.drawCustomSprite("title_screen_air_new", credits.text.center.x, 20 + 20, 0x00, SPRITE_FLAG_PRIO, renderQueue)
		else
			Renderer.drawCustomSprite("title_screen_air", credits.text.center.x, 20 + 20, 0x00, SPRITE_FLAG_PRIO, renderQueue)

		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y - 4*line, "Text/Menu Frameworks", 0xffff8000 + alpha.text, 2, 0, renderQueue, 0, 1)
		Renderer.drawText("sonic_r_credits_small:shadow(1,1,0,1)", credits.text.center.x, credits.text.center.y - 3*line, "AirWay1", 0xffffff00 + alpha.text, 2, 0, renderQueue, 0, 1)
